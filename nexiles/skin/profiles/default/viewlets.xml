<?xml version="1.0"?>
<!-- This file holds the setup configuration of the viewlet managers
     for the "nexiles GmbH Plone skin" skin.
     -->

<object>
  <order manager="plone.portalheader" skinname="nexiles GmbH Plone skin">
    <viewlet name="plone.skip_links"/>
    <viewlet name="plone.personal_bar"/>
    <viewlet name="plone.searchbox"/>
    <viewlet name="plone.app.i18n.locales.languageselector"/>
    <viewlet name="plone.logo"/>
    <viewlet name="plone.global_sections"/>
  </order>

  <hidden manager="plone.portalfooter" skinname="nexiles GmbH Plone skin">
    <viewlet name="plone.footer"/>
    <viewlet name="plone.colophon"/>
  </hidden>
  <hidden manager="plone.portalheader" skinname="nexiles GmbH Plone skin"/>
  <hidden manager="plone.abovecontent" skinname="nexiles GmbH Plone skin">
    <viewlet name="plone.path_bar"/>
  </hidden>
</object>
